        <Button x:Name="NuevoButton" Content="Nuevo documento" Margin="7,7,7,7" Click="NuevoButton_Click"></Button>
        <Button x:Name="ConfButton" Content="Configuración" Margin="7,0,7,7" Click="ConfButton_Click"></Button>
    </StackPanel>
</Window>
